<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.iiuiservices">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IIUIServices"
        tools:ignore="AllowBackup">
        <activity
            android:name=".Ratingbar"
            android:exported="false" />
        <activity
            android:name=".IIUI_Buses_Routes"
            android:exported="false" />
        <activity
            android:name=".Preverices_cgp_resulth"
            android:exported="false" />

        <receiver
            android:name=".ConnectionReceiver"
            android:exported="true">
            <intent-filter>
                <action
                    android:name="android.net.conn.CONNECTIVITY_CHANGE"
                    tools:ignore="BatteryLife" />
            </intent-filter>
        </receiver>

        <activity
            android:name=".IIUI_Contact_list"
            android:exported="false" />
        <activity
            android:name=".Forget_Password"
            android:exported="false" />
        <activity
            android:name=".fyp"
            android:exported="false" />
        <activity
            android:name=".iiui_hostel"
            android:exported="false" />
        <activity
            android:name=".GPA_Calculator"
            android:exported="false" />
        <activity
            android:name=".bscs"
            android:exported="false" />
        <activity
            android:name=".bsit_courses"
            android:exported="false" />
        <activity
            android:name=".BSSE"
            android:exported="false" />
        <activity
            android:name=".cources"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".Home_page"
            android:exported="false"
            android:label="@string/title_activity_home_page"
            android:theme="@style/Theme.IIUIServices.NoActionBar" />
        <activity
            android:name=".Registration"
            android:exported="false" />
        <activity
            android:name=".luncher"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>